{
    "name": "sagar-s-bhedodkar/laravel-modular-routes",
    "description": "A production-ready Laravel package for modular route management with module scaffolding.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sagar Sunil Bhedodkar",
            "email": "sagarbhedodkar456@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "SagarSBhedodkar\\LaravelModularRoutes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SagarSBhedodkar\\LaravelModularRoutes\\Providers\\ModularRouteServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/sagar-s-bhedodkar/laravel-modular-routes/issues",
        "source": "https://github.com/sagar-s-bhedodkar/laravel-modular-routes"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sagar-s-bhedodkar/laravel-modular-routes"
        }
    ]
}
